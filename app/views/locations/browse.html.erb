<div class = "container">
  <span style ="color:pink">
    LAT: <%= @lat %>
    LONG: <%= @long %>
  </span>
  <div class = "row">
    <div class = "col-md-1">
      <span class="glyphicon glyphicon-triangle-left" aria-hidden="true"></span>
    </div>
    <div id = "all" class = "center-block">
      <% @photo_hash.each do |inner_hash| %>
          <% url = inner_hash.keys.first %>
          <div id =<%= url %> class = <%= inner_hash[url] %> ></div>
      <% end %>
    </div>
    <div class = "col-md-1">
      <span class="glyphicon glyphicon-triangle-right" aria-hidden="true"></span>
    </div>
  </div>
</div>
